      <a>
        <span class="icon"><i class="fa fa-user-circle" aria-hidden="true"></i></span>
        <span>Mío</span>
      </a>
    </li>
    <li id="team-link" class="<%= 'is-active' if @open_tab.eql?('team') %>">
